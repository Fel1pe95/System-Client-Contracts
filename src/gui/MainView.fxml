            <Button fx:id="buttonClientData" layoutX="12.0" layoutY="197.0" mnemonicParsing="false" onAction="#onButtonClientDataAction" onMouseEntered="#onButtonClientEnteredMouse" onMouseExited="#onButtonExited" prefHeight="25.0" prefWidth="180.0" style="-fx-background-color: #FFA500;" text="Clientes" />
         </children>
      </Pane>
      <Pane fx:id="MainRightPane" layoutX="205.0" prefHeight="500.0" prefWidth="400.0" style="-fx-background-color: #FAFAD2;" />
